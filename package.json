    "url-loader": "^0.5.7",
    "vue-flatpickr": "^2.2.9",
    "vue-loader": "^10.1.1",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.0",
    "vueify": "^9.4.0",
    "watchify": "^3.8.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1"
  },
  "browserify": {
    "browser": {
      "vue": "vue/dist/vue.common"
    },
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}

{
  "name": "poc-vueapp-full",
  "version": "1.0.0",
  "description": "Vue.js POC app for products portfolio management",
  "author": "aelkz <raphael.alex@gmail.com>",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o build/build.js",
    "serve": "json-server --watch src/database/db.json --static ./ --port 8080",
    "build": "cross-env NODE_ENV=production browserify src/main.js | uglifyjs -c warnings=false -m > build/build.js",
    "dev": "npm-run-all --parallel watchify serve",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "clean": "npm prune",
    "predeploy": "npm run build",
    "deploy": "surge --project . --domain app.surge.sh",
    "test-dev": "node build/dev-server.js",
    "test-dev2": "node build/dev-server.js --inline --hot",
    "security-jwt": "node security-jwt/server.js"
  },
  "dependencies": {
    "bulma": "^0.3.1",
    "font-awesome": "^4.7.0",
    "sweetalert": "^1.1.3",
    "vue": "^2.1.0",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.16.0",
    "browserify": "^13.3.0",
    "browserify-hmr": "^0.3.5",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.1.0",
    "cors": "^2.8.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.25.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.1.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "function-bind": "^1.0.2",
    "html-webpack-plugin": "^2.8.1",
    "http-proxy-middleware": "^0.17.2",
    "http-server": "^0.9.0",
    "inject-loader": "^2.0.1",
    "intl-tel-input": "^10.0.7",
    "jquery": "^3.1.1",
    "jquery-mask-plugin": "^1.14.9",
    "json-loader": "^0.5.4",
    "json-server": "^0.9.4",
    "jsonwebtoken": "^7.2.1",
    "lodash": "^4.17.4",
    "lolex": "^1.4.0",
    "mocha": "^3.1.0",
    "morgan": "^1.7.0",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.1",
